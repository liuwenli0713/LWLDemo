# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'LWLDemo' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod 'LWLCustomKit', :path => 'Lib/LWLCustomKit'
  # Pods for LWLDemo

  target 'LWLDemoTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'LWLDemoUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
